[Unit]
Description=Gunicorn service for Airport Transfer Dev Backend API
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/dev/airporttransfer/backend
ExecStart=/home/ubuntu/dev/airporttransfer/backend/venv/bin/gunicorn core.wsgi:application -b 127.0.0.1:8020 --workers 3 --timeout=60
[Install]
WantedBy=multi-user.target